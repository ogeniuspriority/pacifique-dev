<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>START</title>
    <link rel="stylesheet" href="../css/starting.css" />
    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.13.1/css/all.min.css"
      integrity="sha512-xA6Hp6oezhjd6LiLZynuukm80f8BoZ3OpcEYaqKoCV3HKQDrYjDE1Gu8ocxgxoXmwmSzM4iqPvCsOkQNiu41GA=="
      crossorigin="anonymous"
    />
  </head>
  <script>
    const ROOM_ID = '<%=roomId %>';
  </script>
  <script
    src="https://unpkg.com/peerjs@1.3.1/dist/peerjs.min.js"
    defer
  ></script>
  <script src="/socket.io/socket.io.js" defer></script>
  <script async src="../js/video.js" defer></script>
  <body>
    <div class="container">
      <div id="video_grid"></div>
      <!-- <video id="video" src="" autoplay></video> -->
      <div class="start-container">
        <div class="start">
          <input type="text" placeholder="Enter the user link here" />
          <a href="/call/video"><button>Call</button></a>
        </div>
      </div>
      <div class="buttons">
        <div class="first">
          <div class="button"><i class="fas fa-microphone-slash"></i></div>
          <div class="button"><i class="fas fa-video"></i></div>
          <div class="button" id="share">
            <img src="../../images/screen_share.svg" alt="" />
          </div>
          <a href="/userpage"
            ><div class="button end"><i class="fas fa-phone"></i></div
          ></a>
        </div>
        <div class="second button">
          <i class="fas fa-cog"></i>
          <i class="fas fa-ellipsis-h"></i>
        </div>
      </div>
    </div>
  </body>
</html>
